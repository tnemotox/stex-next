{
  "name": "stex-next",
  "description": "Japan Stocks Analyzer",
  "version": "1.0.0",
  "author": "t.nemoto.x",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "./node_modules/.bin/rimraf ./target/generated-webapp/resources/build",
    "build": "run-s build:dev",
    "build:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --progress",
    "build:prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress",
    "watch": "run-s watch:dev",
    "watch:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --progress --watch",
    "watch:prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --watch",
    "dev": "run-p webpack-dev-server",
    "dev:mock": "run-p webpack-dev-server mock-server",
    "webpack-dev-server": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --hot",
    "mock-server": "node mock/config.js",
    "lint": "run-s --continue-on-error lint:eslint lint:stylelint lint:htmlhint",
    "lint:checkstyle": "run-s --continue-on-error lint:eslint-checkstyle lint:stylelint-checkstyle lint:htmlhint-checkstyle",
    "lint:fix": "run-s --continue-on-error lint:eslint-fix",
    "lint:eslint": "./node_modules/.bin/eslint 'src/main/webapp/**/*.js' 'src/main/webapp/**/*.vue' 'src/main/webapp/**/*.html'"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.4.5",
    "highcharts": "^6.1.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "moment-timezone": "0.5.21",
    "query-string": "^6.1.0",
    "sanitize-html": "^1.18.2",
    "vue": "^2.5.16",
    "vue-highcharts": "0.0.10",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.3.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "es6-promise": "^4",
    "eslint": "^4.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-vue": "^4.7.0",
    "file-loader": "^1.1.11",
    "json-server": "^0.14.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "^4.1.3"
  }
}
