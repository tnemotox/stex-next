root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.xml]
indent_style = space
indent_size = 2

[*.java]
indent_style = space
indent_size = 4

[*.js]
indent_style = space
indent_size = 2

[*.vue]
indent_style = space
indent_size = 2
